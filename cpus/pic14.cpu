<cpudl endian="big">
<cpu name="pic14"/>
<cpu name="pic14e" extends="pic14">
 <feature name="enhanced"/>
</cpu>

<style>
 <property name="base" value="16"/>
 <property name="prefix" value="0x"/>
 <property name="suffix" value=""/>
 <property name="whitespace" value=" "/>
 <select class="daddr">
  <property name="width" value="2"/>
 </select>
 <select class="paddr">
  <property name="width" value="4"/>
 </select>
 <select class="ofs">
  <property name="width" value="2"/>
 </select>
 <select class="imm">
  <property name="width" value="2"/>
 </select>
 <select class="bit">
  <property name="base" value="10"/>
  <property name="prefix" value=""/>
 </select>
</style>

<register name="W" size="8"/>
<register name="SP" size="3"/>
<register name="PC" size="13"/>
<register name="C" size="1"/>

<define name="ignore1">
 <require name="enhanced">
  <fragment>
   <pattern>
    <const>0</const>
   </pattern>
  </fragment>
 </require>

 <require name="!enhanced">
  <fragment>
   <var name="x">
    <integer size="1"/>
   </var>
   <pattern>
    <ref name="x"/>
   </pattern>
  </fragment>
 </require>
</define>

<define name="ignore2">
 <require name="enhanced">
  <fragment>
   <pattern>
    <const>00</const>
   </pattern>
  </fragment>
 </require>

 <require name="!enhanced">
  <fragment>
   <var name="x">
    <integer size="2"/>
   </var>
   <pattern>
    <ref name="x"/>
   </pattern>
  </fragment>
 </require>
</define>

<define name="tris-reg">
 <fragment>
  <pattern>
   <const>101</const>
  </pattern>
  <phrase>
   <literal>5</literal>
  </phrase>
  <effect>
   <const>0x8C</const>
  </effect>
 </fragment>

 <fragment>
  <pattern>
   <const>110</const>
  </pattern>
  <phrase>
   <literal>6</literal>
  </phrase>
  <effect>
   <const>0x8D</const>
  </effect>
 </fragment>

 <fragment>
  <pattern>
   <const>111</const>
  </pattern>
  <phrase>
   <literal>7</literal>
  </phrase>
  <effect>
   <const>0x8E</const>
  </effect>
 </fragment>
</define>

<define name="fsr-reg">
 <fragment>
  <pattern>
   <const>0</const>
  </pattern>
  <phrase>
   <literal>FSR0</literal>
  </phrase>
  <effect>
   <memory size="16" encoding="u" endian="little">
    <const>0x04</const>
   </memory>
  </effect>
 </fragment>

 <fragment>
  <pattern>
   <const>1</const>
  </pattern>
  <phrase>
   <literal>FSR1</literal>
  </phrase>
  <effect>
   <memory size="16" encoding="u" endian="little">
    <const>0x06</const>
   </memory>
  </effect>
 </fragment>
</define>

<define name="fsr-id-ea">
 <fragment>
  <var name="fsr">
   <ref name="fsr-reg"/>
  </var>
  <pattern>
   <ref name="fsr"/>
   <const>00</const>
  </pattern>
  <phrase>
   <literal>++</literal>
   <ref name="fsr"/>
  </phrase>
  <effect>
   <assign>
    <ref name="fsr"/>
    <add>
     <ref name="fsr"/>
     <const>1</const>
    </add>
   </assign>
   <ref name="fsr"/>
  </effect>
 </fragment>

 <fragment>
  <var name="fsr">
   <ref name="fsr-reg"/>
  </var>
  <pattern>
   <ref name="fsr"/>
   <const>01</const>
  </pattern>
  <phrase>
   <literal>--</literal>
   <ref name="fsr"/>
  </phrase>
  <effect>
   <assign>
    <ref name="fsr"/>
    <sub>
     <ref name="fsr"/>
     <const>1</const>
    </sub>
   </assign>
   <ref name="fsr"/>
  </effect>
 </fragment>

 <fragment>
  <var name="fsr">
   <ref name="fsr-reg"/>
  </var>
  <pattern>
   <ref name="fsr"/>
   <const>10</const>
  </pattern>
  <phrase>
   <ref name="fsr"/>
   <literal>++</literal>
  </phrase>
  <effect>
   <temp name="fsr">
    <ref name="fsr"/>
   </temp>
   <assign>
    <ref name="fsr"/>
    <add>
     <ref name="fsr"/>
     <const>1</const>
    </add>
   </assign>
   <temp name="fsr"/>
  </effect>
 </fragment>

 <fragment>
  <var name="fsr">
   <ref name="fsr-reg"/>
  </var>
  <pattern>
   <ref name="fsr"/>
   <const>11</const>
  </pattern>
  <phrase>
   <ref name="fsr"/>
   <literal>--</literal>
  </phrase>
  <effect>
   <temp name="fsr">
    <ref name="fsr"/>
   </temp>
   <assign>
    <ref name="fsr"/>
    <sub>
     <ref name="fsr"/>
     <const>1</const>
    </sub>
   </assign>
   <temp name="fsr"/>
  </effect>
 </fragment>
</define>

<define name="fsr-ofs-ea">
 <fragment>
  <var name="fsr">
   <ref name="fsr-reg"/>
  </var>
  <var name="k">
   <integer class="ofs" size="6" encoding="2c"/>
  </var>
  <pattern>
   <ref name="fsr"/>
   <ref name="k"/>
  </pattern>
  <phrase>
   <ref name="k"/>
   <literal>[</literal>
   <ref name="fsr"/>
   <literal>]</literal>
  </phrase>
  <effect>
   <add>
    <ref name="fsr"/>
    <ref name="k"/>
   </add>
  </effect>
 </fragment>
</define>

