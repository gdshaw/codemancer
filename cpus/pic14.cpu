<define name="return-instr">
 <fragment>
  <pattern>
   <const>00000000</const>
   <const>00001000</const>
  </pattern>
  <phrase>
   <literal>RETURN</literal>
  </phrase>
  <phrase></phrase>
  <effect>
   <assign>
    <register name="SP"/>
    <sub>
     <register name="SP"/>
     <const>1</const>
    </sub>
   </assign>
   <assign>
    <register name="PC"/>
    <memory as="stack">
     <register name="SP"/>
    </memory>
   </assign>
  </effect>
 </fragment>
</define>

<define name="retfie-instr">
 <fragment>
  <pattern>
   <const>00000000</const>
   <const>00001001</const>
  </pattern>
  <phrase>
   <literal>RETFIE</literal>
  </phrase>
  <phrase></phrase>
  <effect>
   <assign>
    <register name="SP"/>
    <sub>
     <register name="SP"/>
     <const>1</const>
    </sub>
   </assign>
   <assign>
    <register name="PC"/>
    <memory as="stack">
     <register name="SP"/>
    </memory>
   </assign>
   <assign>
    <memory size="8" encoding="u">
     <const>0x0B</const>
    </memory>
    <or>
     <memory size="8" encoding="u">
      <const>0x0B</const>
     </memory>
     <const>0x80</const>
    </or>
   </assign>
  </effect>
 </fragment>
</define>

<define name="callw-instr">
 <fragment>
  <pattern>
   <const>00000000</const>
   <const>00001010</const>
  </pattern>
  <phrase>
   <literal>CALLW</literal>
  </phrase>
  <phrase></phrase>
  <effect>
   <assign>
    <memory as="stack">
     <register name="SP"/>
    </memory>
    <register name="PC"/>
   </assign>
   <assign>
    <register name="SP"/>
    <add>
     <register name="SP"/>
     <const>1</const>
    </add>
   </assign>
   <assign>
    <register name="PC"/>
    <or>
     <shift width="13" method="logical">
      <and>
       <memory size="8">
        <const>0x0A</const>
       </memory>
       <const>0x7F</const>
      </and>
      <const>8</const>
     </shift>
     <register name="W"/>
    </or>
   </assign>
  </effect>
 </fragment>
</define>

